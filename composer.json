{
    "name": "jamarcer/symfony-messenger-logging",
    "description": "Symfony Messenger Logging",
    "version": "1.0.0",
    "license": "MIT",
    "type": "library",
    "authors": [
    {
        "name": "Jesús Martínez",
        "email": "jamarcer@gmail.com"
    }
    ],
    "autoload": {
        "psr-4": {
            "Jamarcer\\DDDLogging\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Jamarcer\\Tests\\DDDLogging\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "monolog/monolog": "^2.1",
        "jamarcer/ddd-framework": "dev-main",
        "symfony/http-kernel": "5.2.*",
        "symfony/messenger": "^v5.2",
        "symfony/http-foundation": "5.2.*",
        "symfony/stopwatch": "5.2.*",
        "guzzlehttp/guzzle": "^6.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "dg/bypass-finals": "^1.1"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jamarcer/ddd-framework"
        }
    ]
}
